    "semantic-release": "19.0.2"
  },
  "peerDependencies": {
    "typescript": ">=4"
  },
  "author": "Filip Tammerg√•rd",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "tsconfig.json"
  ],
  "engines": {
    "node": "14 || 16"
  },
  "keywords": [
    "typescript",
    "config",
    "tsconfig"
  ],
  "license": "MIT",
  "homepage": "https://github.com/einride/tsconfig",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/tsconfig.git"
  },
  "bugs": {
