{
  "name": "@einride/tsconfig",
  "version": "0.0.0-development",
  "description": "Einride's default TSconfig",
  "scripts": {
    "commitlint": "commitlint --from origin/main --to HEAD",
    "git-verify-nodiff": "./scripts/git-verify-nodiff.bash",
    "prettier": "prettier --write .",
    "review": "yarn install --frozen-lockfile && yarn commitlint && yarn prettier && yarn git-verify-nodiff"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "16.2.1",
    "@einride/prettier-config": "1.2.0",
    "@types/node": "16.11.26",
    "commitlint": "16.2.3",
    "prettier": "2.5.1",
    "semantic-release": "19.0.2"
  },
  "peerDependencies": {
    "typescript": ">=4"
  },
  "author": "Filip Tammerg√•rd",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "tsconfig.json"
  ],
  "engines": {
    "node": "14 || 16"
  },
  "keywords": [
    "typescript",
    "config",
    "tsconfig"
  ],
  "license": "MIT",
  "homepage": "https://github.com/einride/tsconfig",
  "repository": {
    "type": "git",
    "url": "https://github.com/einride/tsconfig.git"
  },
  "bugs": {
    "email": "open-source@einride.tech",
    "url": "https://github.com/einride/tsconfig/issues"
  }
